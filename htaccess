
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:harman-cert.com
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?harman-cert.com
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
